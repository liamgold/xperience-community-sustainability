{
  "name": "sustainability-web-admin",
  "version": "1.0.0",
  "description": "Custom Kentico Xperience administration project.",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode development",
    "dev:buildlike": "webpack serve --mode development --env buildlike=true",
    "build": "npm run build:admin && npm run build:resource-checker",
    "build:admin": "webpack --mode=production",
    "build:resource-checker": "webpack --config webpack.resource-checker.config.js"
  },
  "dependencies": {
    "@kentico/xperience-admin-base": "30.4.2",
    "@kentico/xperience-admin-components": "30.4.2",
    "@tgwf/co2": "^0.16.0",
    "css-loader": "^7.1.2",
    "postcss-loader": "^8.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "style-loader": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@kentico/xperience-webpack-config": "30.4.2",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "babel-loader": "^10.0.0",
    "postcss": "^8.5.6",
    "webpack": "5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react",
    "json-joy": ">= 11.19.0"
  },
  "browserslist": [
    "> .2% and last 3 versions",
    "not op_mini all",
    "not IE 11"
  ]
}
